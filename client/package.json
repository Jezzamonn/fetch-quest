{
  "name": "untitled-dog-game-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "watch": {
    "webpack": "js/*.js",
    "html": {
      "patterns": "index.html",
      "extensions": "html",
      "ignore": "build/*.html"
    },
    "css": {
      "patterns": "css/*.css",
      "extensions": "css"
    }
  },
  "scripts": {
    "start": "http-server",
    "webpack": "webpack || exit 1",
    "html": "cp index.html build/index.html",
    "css": "rm -r build/css; cp -r css build/css",
    "build": "npm run webpack && npm run html && npm run css",
    "watch": "npm-watch",
    "reload": "reload -b -d build"
  },
  "author": "Jezzamon",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.14.7",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "http-server": "^0.12.3",
    "npm-watch": "^0.10.0",
    "reload": "^3.2.0",
    "webpack": "^5.43.0",
    "webpack-cli": "^4.7.2",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "socket.io-client": "^4.1.2"
  }
}
